<?xml version="1.0" ?>

<container xmlns="http://www.symfony-project.org/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.symfony-project.org/schema/dic/services http://www.symfony-project.org/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="doctrine.orm.default_entity_manager">default</parameter>
        <parameter key="doctrine.orm.metadata_cache_driver">array</parameter>
        <parameter key="doctrine.orm.query_cache_driver">array</parameter>
        <parameter key="doctrine.orm.result_cache_driver">array</parameter>
        <parameter key="doctrine.orm.configuration_class">Doctrine\ORM\Configuration</parameter>
        <parameter key="doctrine.orm.entity_manager_class">Doctrine\ORM\EntityManager</parameter>

        <!-- proxies -->
        <parameter key="doctrine.orm.proxy_namespace">Proxies</parameter>
        <parameter key="doctrine.orm.auto_generate_proxy_classes">false</parameter>

        <!-- cache -->
        <parameter key="doctrine.orm.cache.array_class">Doctrine\Common\Cache\ArrayCache</parameter>
        <parameter key="doctrine.orm.cache.apc_class">Doctrine\Common\Cache\ApcCache</parameter>
        <parameter key="doctrine.orm.cache.memcache_class">Doctrine\Common\Cache\MemcacheCache</parameter>
        <parameter key="doctrine.orm.cache.memcache_host">localhost</parameter>
        <parameter key="doctrine.orm.cache.memcache_port">11211</parameter>
        <parameter key="doctrine.orm.cache.memcache_instance_class">Memcache</parameter>
        <parameter key="doctrine.orm.cache.xcache_class">Doctrine\Common\Cache\XcacheCache</parameter>

        <!-- metadata -->
        <parameter key="doctrine.orm.metadata.driver_chain_class">Doctrine\ORM\Mapping\Driver\DriverChain</parameter>
        <parameter key="doctrine.orm.metadata.annotation_class">Doctrine\ORM\Mapping\Driver\AnnotationDriver</parameter>
        <parameter key="doctrine.orm.metadata.annotation_reader_class">Doctrine\Common\Annotations\AnnotationReader</parameter>
        <parameter key="doctrine.orm.metadata.annotation_default_namespace">Doctrine\ORM\Mapping\</parameter>
        <parameter key="doctrine.orm.metadata.xml_class">Doctrine\ORM\Mapping\Driver\XmlDriver</parameter>
        <parameter key="doctrine.orm.metadata.yml_class">Doctrine\ORM\Mapping\Driver\YamlDriver</parameter>

        <!-- directories -->
        <parameter key="doctrine.orm.metadata_driver.mapping_dirs" type="collection"></parameter>
        <parameter key="doctrine.orm.xml_mapping_dirs">%doctrine.orm.metadata_driver.mapping_dirs%</parameter>
        <parameter key="doctrine.orm.yml_mapping_dirs">%doctrine.orm.metadata_driver.mapping_dirs%</parameter>
        <parameter key="doctrine.orm.metadata_driver.entity_dirs" type="collection"></parameter>

        <!-- console commands -->
        <parameter key="doctrine.orm.console.command.clear_metadata_cache.class">Symfony\Bundle\DoctrineBundle\Command\ClearMetadataCacheDoctrineCommand</parameter>
        <parameter key="doctrine.orm.console.command.clear_query_cache.class">Symfony\Bundle\DoctrineBundle\Command\ClearQueryCacheDoctrineCommand</parameter>
        <parameter key="doctrine.orm.console.command.clear_result_cache.class">Symfony\Bundle\DoctrineBundle\Command\ClearResultCacheDoctrineCommand</parameter>
        <parameter key="doctrine.orm.console.command.convert_doctrine1_schema.class">Symfony\Bundle\DoctrineBundle\Command\ConvertDoctrine1SchemaDoctrineCommand</parameter>
        <parameter key="doctrine.orm.console.command.convert_mapping.class">Symfony\Bundle\DoctrineBundle\Command\ConvertMappingDoctrineCommand</parameter>
        <parameter key="doctrine.orm.console.command.create_schema.class">Symfony\Bundle\DoctrineBundle\Command\CreateSchemaDoctrineCommand</parameter>
        <parameter key="doctrine.orm.console.command.drop_schema.class">Symfony\Bundle\DoctrineBundle\Command\DropSchemaDoctrineCommand</parameter>
        <parameter key="doctrine.orm.console.command.ensure_production_settings.class">Symfony\Bundle\DoctrineBundle\Command\EnsureProductionSettingsDoctrineCommand</parameter>
        <parameter key="doctrine.orm.console.command.generate_entities.class">Symfony\Bundle\DoctrineBundle\Command\GenerateEntitiesDoctrineCommand</parameter>
        <parameter key="doctrine.orm.console.command.generate_entity.class">Symfony\Bundle\DoctrineBundle\Command\GenerateEntityDoctrineCommand</parameter>
        <parameter key="doctrine.orm.console.command.generate_proxies.class">Symfony\Bundle\DoctrineBundle\Command\GenerateProxiesDoctrineCommand</parameter>
        <parameter key="doctrine.orm.console.command.generate_repositories.class">Symfony\Bundle\DoctrineBundle\Command\GenerateRepositoriesDoctrineCommand</parameter>
        <parameter key="doctrine.orm.console.command.import_mapping.class">Symfony\Bundle\DoctrineBundle\Command\ImportMappingDoctrineCommand</parameter>
        <parameter key="doctrine.orm.console.command.load_data_fixtures.class">Symfony\Bundle\DoctrineBundle\Command\LoadDataFixturesDoctrineCommand</parameter>
        <parameter key="doctrine.orm.console.command.run_dql.class">Symfony\Bundle\DoctrineBundle\Command\RunDqlDoctrineCommand</parameter>
        <parameter key="doctrine.orm.console.command.update_schema.class">Symfony\Bundle\DoctrineBundle\Command\UpdateSchemaDoctrineCommand</parameter>
    </parameters>

    <services>
        <!--- Annotation Metadata Driver Service -->
        <service id="doctrine.orm.metadata_driver.annotation" class="%doctrine.orm.metadata.annotation_class%">
            <argument type="service" id="doctrine.orm.metadata_driver.annotation.reader" />
            <argument>%doctrine.orm.metadata_driver.entity_dirs%</argument>
        </service>

        <service id="doctrine.orm.metadata_driver.annotation.reader" class="%doctrine.orm.metadata.annotation_reader_class%">
            <call method="setAnnotationNamespaceAlias">
              <argument>Doctrine\ORM\Mapping\</argument>
              <argument>orm</argument>
            </call>
        </service>

        <!--- XML Metadata Driver Service -->
        <service id="doctrine.orm.metadata_driver.xml" class="%doctrine.orm.metadata.xml_class%">
            <argument>%doctrine.orm.metadata_driver.mapping_dirs%</argument>
        </service>

        <!--- YAML Metadata Driver Service -->
        <service id="doctrine.orm.metadata_driver.yml" class="%doctrine.orm.metadata.yml_class%">
            <argument>%doctrine.orm.metadata_driver.mapping_dirs%</argument>
        </service>

        <!-- console commands -->
        <service id="doctrine.orm.console.command.clear_metadata_cache" class="%doctrine.orm.console.command.clear_metadata_cache.class%">
            <tag name="console.command" />
        </service>

        <service id="doctrine.orm.console.command.clear_query_cache" class="%doctrine.orm.console.command.clear_query_cache.class%">
            <tag name="console.command" />
        </service>

        <service id="doctrine.orm.console.command.clear_result_cache" class="%doctrine.orm.console.command.clear_result_cache.class%">
            <tag name="console.command" />
        </service>

        <service id="doctrine.orm.console.command.convert_doctrine1_schema" class="%doctrine.orm.console.command.convert_doctrine1_schema.class%">
            <tag name="console.command" />
        </service>

        <service id="doctrine.orm.console.command.convert_mapping" class="%doctrine.orm.console.command.convert_mapping.class%">
            <tag name="console.command" />
        </service>

        <service id="doctrine.orm.console.command.create_schema" class="%doctrine.orm.console.command.create_schema.class%">
            <tag name="console.command" />
        </service>

        <service id="doctrine.orm.console.command.drop_schema" class="%doctrine.orm.console.command.drop_schema.class%">
            <tag name="console.command" />
        </service>

        <service id="doctrine.orm.console.command.ensure_production_settings" class="%doctrine.orm.console.command.ensure_production_settings.class%">
            <tag name="console.command" />
        </service>

        <service id="doctrine.orm.console.command.generate_entities" class="%doctrine.orm.console.command.generate_entities.class%">
            <tag name="console.command" />
        </service>

        <service id="doctrine.orm.console.command.generate_entity" class="%doctrine.orm.console.command.generate_entity.class%">
            <tag name="console.command" />
        </service>

        <service id="doctrine.orm.console.command.generate_proxies" class="%doctrine.orm.console.command.generate_proxies.class%">
            <tag name="console.command" />
        </service>

        <service id="doctrine.orm.console.command.generate_repositories" class="%doctrine.orm.console.command.generate_repositories.class%">
            <tag name="console.command" />
        </service>

        <service id="doctrine.orm.console.command.import_mapping" class="%doctrine.orm.console.command.import_mapping.class%">
            <tag name="console.command" />
        </service>

        <service id="doctrine.orm.console.command.load_data_fixtures" class="%doctrine.orm.console.command.load_data_fixtures.class%">
            <tag name="console.command" />
        </service>

        <service id="doctrine.orm.console.command.run_dql" class="%doctrine.orm.console.command.run_dql.class%">
            <tag name="console.command" />
        </service>
        
        <service id="doctrine.orm.console.command.update_schema" class="%doctrine.orm.console.command.update_schema.class%">
            <tag name="console.command" />
        </service>
    </services>
</container>
